{
  "functions": {
    "app/[locale]/(routes)/auth/register/page.tsx": {
      "runtime": "edge"
    },
    "app/[locale]/(routes)/auth/login/page.tsx": {
      "runtime": "edge"
    }
  },
  "headers": [
    {
      "source": "/locales/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*\\.(js|css|woff2|png|jpg|jpeg|gif|svg|ico))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=60, stale-while-revalidate"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/register",
      "destination": "/en/auth/register"
    },
    {
      "source": "/login",
      "destination": "/en/auth/login"
    }
  ],
  "regions": ["iad1", "fra1", "sfo1"],
  "framework": "nextjs"
} 